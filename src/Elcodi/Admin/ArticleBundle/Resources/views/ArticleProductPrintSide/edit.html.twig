{% extends 'AdminCoreBundle::layout.html.twig' %}


{% set elcodi_context = {
    entity_type: 'articleProduct'
} %}


{% block body %}

    {% if articleProduct.articleProductPrintSide.id %}
        {{
            render(url('admin_article_product_print_side_edit_component', {
                articleProductId: articleProduct.id,
                id: articleProduct.articleProductPrintSide.id,
            }))
        }}
    {% else %}
        {{
            render(url('admin_article_product_print_side_new_component', {
                articleProductId: articleProduct.id,
            }))
        }}
    {% endif %}

{% endblock body %}
